    "suggest": {
        "swiftmailer/swiftmailer": "安装后您可以使用插件中的邮件发送器以及验证码发送器",
        "phpoffice/phpspreadsheet": "安装后您可以使用插件对Office文件进行操作"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "Ipuppet\\Jade\\": "src/"
